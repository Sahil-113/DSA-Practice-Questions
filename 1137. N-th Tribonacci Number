class Solution {
public:
    int t[38];
    int tribonacci1(int n) {
        if(n==0){
            return 0;
        }
        else if(n==1){
            return 1;
        }
        else if(n==2){
            return 1;
        }
        if(t[n] != -1){
            return t[n];
        }

        return t[n]=tribonacci1(n-1)+tribonacci1(n-2)+tribonacci1(n-3);
        
    }
    int tribonacci(int n){
        memset(t,-1,sizeof(t));
        return tribonacci1(n);
    }
};
